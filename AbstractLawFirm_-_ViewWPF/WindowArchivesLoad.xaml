<Window x:Class="AbstractLawFirm___ViewWPF.WindowArchivesLoad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbstractLawFirm___ViewWPF"
        mc:Ignorable="d"
        Title="WindowArchivesLoad" Height="528" Width="384">
    <Grid Loaded="Grid_Loaded">
        <DataGrid x:Name="dataGridView" HorizontalAlignment="Left" Height="446" Margin="10,41,0,0" VerticalAlignment="Top" Width="356">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="Archive" Width="120"/>
                <DataGridTextColumn x:Name="Blank" Width="120"/>
                <DataGridTextColumn x:Name="Count" Width="120"/>
            </DataGrid.Columns>
            
        </DataGrid>
        <Button x:Name="buttonSaveToExcel" Content="Сохранить в Excel" HorizontalAlignment="Left" Height="26" Margin="10,10,0,0" VerticalAlignment="Top" Width="144" Click="ButtonSaveToExcel_Click"/>

    </Grid>
</Window>
